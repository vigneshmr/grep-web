{
  "manifest_version": 3,
  "name": "Grep Web",
  "version": "1.0",
  "description": "Extension to grep and show lines on the site that you visit",

  "content_scripts": [
    {
      "js": [
        "scripts/content.js"
      ],
      "matches": [
        "*://*.redfin.com/*"
      ],
      "run_at":"document_end"
    }
  ]
}
