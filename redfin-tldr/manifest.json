{
  "manifest_version": 3,
  "name": "Redfin TLDR",
  "version": "1.1.3",
  "description": "Tiny extension to grep and show important info on redfin for quick tldr",

  "content_scripts": [
    {
      "js": [
        "scripts/content.js"
      ],
      "matches": [
        "https://www.redfin.com/*"
      ],
      "run_at":"document_end"
    }
  ]
}
